{
  "name": "next-latest-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:unocss": "unocss --out-file \"src/app/(app)/uno.css\" \"./**/*.tsx\" --watch",
    "dev:next": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:next:turbo": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev": "run-p dev:unocss dev:next:turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build:unocss": "unocss --out-file \"src/app/(app)/uno.css\" \"./**/*.tsx\"",
    "build:next": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build": "payload generate:types && pnpm build:unocss && pnpm build:next",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "typecheck": "tsc",
    "ci": "payload migrate && pnpm build",
    "generate:types": "payload generate:types",
    "standalone-script": "node ./src/scripts/standalone-script.js"
  },
  "engines": {
    "node": "^18.20.2 || >=20.6.0"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "beta",
    "@payloadcms/db-postgres": "beta",
    "@payloadcms/next": "beta",
    "@payloadcms/plugin-seo": "beta",
    "@payloadcms/richtext-lexical": "beta",
    "@payloadcms/storage-s3": "beta",
    "@payloadcms/ui": "beta",
    "accept-language": "^3.0.18",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "i18next": "^23.11.5",
    "lexical": "^0.15.0",
    "next": "14.3.0-canary.68",
    "npm-run-all": "^4.1.5",
    "payload": "beta",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@iconify-json/teenyicons": "^1.1.9",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@unocss/cli": "^0.60.3",
    "@unocss/preset-web-fonts": "^0.60.3",
    "@unocss/reset": "^0.60.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "typescript": "^5.4.5",
    "unocss": "^0.60.3"
  }
}
